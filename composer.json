{
    "name": "daika7ana/sqiz.me",
    "description": "My own personal take on a URL shortener.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "GPL-3.0",
    "type": "project",
    "require": {
        "php": ">=8.0.2",
        "laravel/lumen-framework": "^9.0",
        "vlucas/phpdotenv": "^5.2",
        "laravel/helpers": "^1.2",
        "bepsvpt/secure-headers": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "laravel/tinker": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
